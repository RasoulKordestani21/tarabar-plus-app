{
  "expo": {
    "owner": "rs_53",
    "name": "tarabar-plus",
    "slug": "tarbarplus-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vector.png",
    "scheme": "tarabar-plus",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rs53.tarabarplusfrontend",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["tarabar-plus"]
          }
        ]
      }
    },
    "android": {
      "buildToolsVersion": "33.0.0",
      "compileSdkVersion": 33,
      "targetSdkVersion": 33,
      "kotlinVersion": "1.9.25",
      "enableProguardInReleaseBuilds": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vector.png",
        "backgroundColor": "#003366"
      },
      "package": "com.rs_53.tarabarplusfrontend",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "tarabar-plus",
              "host": "payment-verify"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-build-properties",
      {
        "android": {
          "compileSdkVersion": 36
        }
      }
    ],
    "splash": {
      "image": "./assets/images/vector.png",
      "resizeMode": "contain",
      "backgroundColor": "#003366"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "DEVELOP_URL": "http://192.168.1.104:3000",
      "PRODUCTION_URL": "https://tarabar-plus.ir",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d8b8f99b-a6de-4b4d-ac31-d7808fa6c8b7"
      }
    }
  }
}
